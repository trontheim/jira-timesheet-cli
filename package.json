{
  "name": "jira-timesheet-cli",
  "version": "1.0.0",
  "description": "CLI tool to generate timesheets from Jira worklogs via REST API",
  "type": "module",
  "main": "jira_timesheet_cli.js",
  "bin": {
    "jira-timesheet": "./jira_timesheet_cli.js"
  },
  "scripts": {
    "start": "node jira_timesheet_cli.js",
    "build": "tsc",
    "dev": "node --loader ts-node/esm jira_timesheet_cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "jira",
    "timesheet",
    "worklog",
    "cli",
    "rest-api",
    "atlassian"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.3",
    "commander": "^11.1.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitest/coverage-v8": "^3.2.2",
    "@vitest/spy": "^3.2.2",
    "@vitest/ui": "^3.2.2",
    "c8": "^10.1.3",
    "mock-fs": "^5.2.0",
    "msw": "^2.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
